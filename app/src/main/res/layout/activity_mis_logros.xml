<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MisLogros">


    <nl.dionsegijn.konfetti.KonfettiView
        android:id="@+id/viewKonfetti"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/txt_ml_title1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="2dp"
        android:fontFamily="@font/alata"
        android:text="Mis logros"
        android:textColor="#9C6C4B"
        android:textColorHint="#000000"
        android:textSize="34sp"
        app:layout_constraintBottom_toTopOf="@+id/txt_ml_frase"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txt_ml_header" />

    <TextView
        android:id="@+id/txt_ml_header"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginBottom="382dp"
        android:background="#E59E6D"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/imgbtn_ml_atras"
        android:layout_width="80dp"
        android:layout_height="60dp"
        android:layout_marginEnd="331dp"
        android:layout_marginBottom="671dp"
        android:background="#00FFFFFF"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/ic_baseline_arrow_back_24" />

    <ImageButton
        android:id="@+id/imgbtn_ml_logro2"
        android:layout_width="124dp"
        android:layout_height="111dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="427dp"
        android:layout_marginEnd="273dp"
        android:layout_marginBottom="194dp"
        android:background="@drawable/buttons"
        android:scaleType="fitCenter"
        android:src="@drawable/medalla"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="SpeakableTextPresentCheck"
        tools:srcCompat="@drawable/medalla" />

    <ImageButton
        android:id="@+id/imgbtn_ml_favorito2"
        android:layout_width="80dp"
        android:layout_height="36dp"
        android:layout_marginStart="159dp"
        android:layout_marginTop="503dp"
        android:layout_marginEnd="172dp"
        android:layout_marginBottom="192dp"
        android:background="@drawable/favoritos"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_baseline_favorite_border_24" />

    <ImageButton
        android:id="@+id/imgbtn_ml_escuchar2"
        android:layout_width="80dp"
        android:layout_height="36dp"
        android:layout_marginStart="299dp"
        android:layout_marginTop="503dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="192dp"
        android:background="@drawable/buttons"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_baseline_volume_up_24" />

    <TextView
        android:id="@+id/txt_ml_title3"
        android:layout_width="220dp"
        android:layout_height="70dp"
        android:layout_marginStart="158dp"
        android:layout_marginTop="425dp"
        android:layout_marginEnd="33dp"
        android:layout_marginBottom="236dp"
        android:fontFamily="@font/alata"
        android:isScrollContainer="false"
        android:text="Completar &quot;Uso de apartados.&quot;"
        android:textAlignment="center"
        android:textColor="#9C6C4B"
        android:textColorHint="#000000"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ProgressBar
        android:id="@+id/progressBar_ml_logro2"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="0dp"
        android:layout_height="3dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="559dp"
        android:layout_marginEnd="14dp"
        android:layout_marginBottom="169dp"
        android:text='Completar "Uso de apartados."'
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt_ml_frase"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="36dp"
        android:fontFamily="@font/alata"
        android:text="&quot;El éxito es la suma de pequeños esfuerzos que se repiten día tras día.&quot;"
        android:textAlignment="center"
        android:textColor="#3B5463"
        android:textColorHint="#3B5463"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/txt_ml_title2"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/txt_ml_footer"
        android:layout_width="0dp"
        android:layout_height="60dp"
        android:background="#E59E6D"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/progressBar_ml_logro2"
        app:layout_constraintVertical_bias="1.0" />

    <ImageButton
        android:id="@+id/imgbtn_ml_logro1"
        android:layout_width="124dp"
        android:layout_height="111dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="234dp"
        android:layout_marginEnd="267dp"
        android:layout_marginBottom="386dp"
        android:background="@drawable/buttons"
        android:scaleType="fitCenter"
        android:src="@drawable/exito"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@drawable/exito" />

    <ProgressBar
        android:id="@+id/progressBar_ml_logro1"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="371dp"
        android:layout_height="3dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="360dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="368dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt_ml_title2"
        android:layout_width="220dp"
        android:layout_height="70dp"
        android:layout_marginStart="159dp"
        android:layout_marginTop="234dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="427dp"
        android:fontFamily="@font/alata"
        android:isScrollContainer="false"
        android:text="Completar &quot;Primeros pasos.&quot;"
        android:textAlignment="center"
        android:textColor="#9C6C4B"
        android:textColorHint="#000000"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/imgbtn_ml_favorito1"
        android:layout_width="80dp"
        android:layout_height="36dp"
        android:layout_marginStart="159dp"
        android:layout_marginTop="309dp"
        android:layout_marginEnd="172dp"
        android:layout_marginBottom="386dp"
        android:background="@drawable/favoritos"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_baseline_favorite_border_24" />

    <ImageButton
        android:id="@+id/imgbtn_ml_escuchar"
        android:layout_width="80dp"
        android:layout_height="36dp"
        android:layout_marginStart="299dp"
        android:layout_marginTop="309dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="386dp"
        android:background="@drawable/buttons"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_baseline_volume_up_24" />

    <androidx.constraintlayout.widget.Group
        android:id="@+id/group"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:constraint_referenced_ids="viewKonfetti,imgbtn_ml_logro2,imgbtn_ml_favorito2,imgbtn_ml_escuchar2,txt_ml_title3,progressBar_ml_logro2,txt_ml_frase,txt_ml_footer,imgbtn_ml_logro1,progressBar_ml_logro1,txt_ml_title2,imgbtn_ml_favorito1,imgbtn_ml_escuchar"
        app:layout_constraintBottom_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>